<!-- page content -->
<div class="right_col" role="main">
  <div class="">
    <div class="clearfix"></div>
    <div class="row">
      <div class="col-md-12 col-sm-12">
        <div class="x_panel">
          <div class="x_title">
            <h1>Programming Contest /<small>Register a new team</small></h1>
            <ul class="nav navbar-right panel_toolbox">
              <li>
                <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
              </li>
              <li class="dropdown">
                <a
                  href="#"
                  class="dropdown-toggle"
                  data-toggle="dropdown"
                  role="button"
                  aria-haspopup="true"
                  aria-expanded="false"
                  ><i class="fa fa-wrench"></i
                ></a>
              </li>
            </ul>

            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <br />

            <% let success="Team has been registered successfully!" %>
            <%if(error.length>0){if(error==success){%>
            <div class="alert alert-success"><%=error%></div>
            <%}else{%>
            <div class="alert alert-danger"><%=error%></div>
            <%}%><%}%>

            <form
              method="POST"
              action="/ProgrammingContest/register"
              id="demo-form2"
              data-parsley-validate
              class="form-horizontal form-label-left"
            >
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <input
                      type="text"
                      name="teamName"
                      id="teamName"
                      class="form-control"
                      placeholder="Team Name"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="institution"
                      id="institution"
                      class="form-control"
                      placeholder="Institution Name"
                    />
                  </div>
                </div>
              </div>

              <br />
              <h3>Coach Info</h3>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <input
                      type="text"
                      name="coach"
                      id="coach"
                      class="form-control"
                      placeholder="Coach Name"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="contactCoach"
                      id="contactCoach"
                      class="form-control"
                      placeholder="Coach Contact No"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="emailCoach"
                      id="emailCoach"
                      class="form-control"
                      placeholder="Coach Email"
                    />
                  </div>
                </div>
              </div>
              <h6>Coach T-Shirt size</h6>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <select
                      class="form-control"
                      id="tshirtCoach"
                      name="tshirtCoach"
                    >
                      <option disabled>... Choose Option ...</option>
                      <option value="M">Medium</option>
                      <option value="L">Large</option>
                      <option value="XL">Extra Large</option>
                    </select>
                  </div>
                </div>
              </div>

              <br />
              <h3>Team Leader Info</h3>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <input
                      type="text"
                      name="leader"
                      id="leader"
                      class="form-control"
                      placeholder="Team Leader Name"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="contactLeader"
                      id="contactLeader"
                      class="form-control"
                      placeholder="Team Leader Contact"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="emailLeader"
                      id="emailLeader"
                      class="form-control"
                      placeholder="Team Leader Email"
                    />
                  </div>
                </div>
              </div>
              <h6>Team Leader T-Shirt size</h6>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <select
                      class="form-control"
                      id="tshirtLeader"
                      name="tshirtLeader"
                    >
                      <option disabled>... Choose Option ...</option>
                      <option value="M">Medium</option>
                      <option value="L">Large</option>
                      <option value="XL">Extra Large</option>
                    </select>
                  </div>
                </div>
              </div>

              <br />
              <h3>Member 1 Info</h3>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <input
                      type="text"
                      name="member1"
                      id="member1"
                      class="form-control"
                      placeholder="Member 1 Name"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="contactMember1"
                      id="contactMember1"
                      class="form-control"
                      placeholder="Member 1 Contact no"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="emailMember1"
                      id="emailMember1"
                      class="form-control"
                      placeholder="Email of Member-1"
                    />
                  </div>
                </div>
              </div>
              <h6>Member 1 Tshirt size</h6>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <select
                      class="form-control"
                      id="tshirtMember1"
                      name="tshirtMember1"
                    >
                      <option disabled>... Choose Option ...</option>
                      <option value="M">Medium</option>
                      <option value="L">Large</option>
                      <option value="XL">Extra Large</option>
                    </select>
                  </div>
                </div>
              </div>

              <br />
              <h3>Member 2 Info</h3>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <input
                      type="text"
                      name="member2"
                      id="member2"
                      class="form-control"
                      placeholder="Member 2 Name"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="contactMember2"
                      id="contactMember2"
                      class="form-control"
                      placeholder="Member 2 Contact"
                    />
                  </div>
                  <div class="col">
                    <input
                      type="text"
                      name="emailMember2"
                      id="emailMember2"
                      class="form-control"
                      placeholder="Member 2 Email"
                    />
                  </div>
                </div>
              </div>

              <h6>Member 2 tshirt size</h6>
              <div class="item form-group">
                <div class="form-row">
                  <div class="col">
                    <select
                      class="form-control"
                      id="tshirtMember2"
                      name="tshirtMember2"
                    >
                      <option disabled>... Choose Option ...</option>
                      <option value="M">Medium</option>
                      <option value="L">Large</option>
                      <option value="XL">Extra Large</option>
                    </select>
                  </div>
                </div>
              </div>

              <div class="ln_solid"></div>
              <div class="item form-group">
                <div class="col-md-6 col-sm-6 offset-md-3">
                  <button class="btn btn-primary" type="reset">Reset</button>
                  <button type="submit" class="btn btn-success">Submit</button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
